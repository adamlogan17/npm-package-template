{
  "out": "./docs",
  "exclude": ["**/node_modules/**/*", "**/tests/**/*"],
  "theme": "default",
  "name": "NPM package",
  "readme": "README.md",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "tsconfig": "tsconfig.json",
  "entryPoints": ["./lib"],
  "entryPointStrategy": "expand"
}
